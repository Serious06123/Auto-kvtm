{
    "name": "auto-tools",
    "description": "Auto Tools",
    "scripts": {
        "appium": "appium --base-path=/wd/hub --log-timestamp --local-timezone --log-no-colors --log-level error:warn --log ./log/appium.txt",
        "app": "node src/server.js",
        "build": "webpack --mode=development --watch",
        "release": "webpack --mode=production",
        "start": "pm2 start ecosystem.config.js",
        "monitor": "pm2 monit",
        "restart": "pm2 restart all",
        "stop": "pm2 stop all",
        "clear": "pm2 delete all",
        "commit": "git-cz"
    },
    "keywords": [
        "Khu Vuon Tren May",
        "Auto Tools"
    ],
    "author": "",
    "license": "ISC",
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "dependencies": {
        "appium": "^2.17.1",
        "axios": "^1.8.4",
        "bluebird": "^3.7.2",
        "buffer": "^6.0.3",
        "express": "^4.21.2",
        "express-async-errors": "^3.1.1",
        "express-ws": "^5.0.2",
        "jimp": "^0.22.12",
        "jmuxer": "^1.2.0",
        "moment": "^2.30.1",
        "multer": "^2.0.2",
        "opencv-wasm": "^4.3.0-10",
        "pm2": "^6.0.5",
        "tesseract.js": "^6.0.1",
        "webdriverio": "^9.12.4"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-react": "^7.26.3",
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-conventional": "^19.8.0",
        "@commitlint/cz-commitlint": "^19.8.0",
        "antd": "^5.24.6",
        "appium-uiautomator2-driver": "^4.2.0",
        "babel-loader": "^9.2.1",
        "commitizen": "^4.3.1",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.2",
        "html-webpack-plugin": "^5.6.3",
        "mini-css-extract-plugin": "^2.9.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "terser-webpack-plugin": "^5.3.14",
        "webpack": "^5.99.5",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.2.1"
    }
}
